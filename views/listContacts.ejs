<!DOCTYPE html>
  <html lang="es">
    <head>
      <meta charset="UTF-8">
      <!--<meta http-equiv="X-UA-Compatible" content="IE=edge">-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <%- include('partials/enlaces' );%>
      <title>Lista de Contactos</title>
    </head>
    <body>
      <header>
        <%- include('partials/header'); %>
      </header>
      <p></p><p></p>
      <h2><%=contactsBooksL.name%></h2>
      <div>
        <table class="table table-striped">
          <thead class="thead-dark">
            <tr>
              <th scope="col">Nombre</th>
              <th scope="col">E-mail</th>
              <th scope="col">Tel√©fono</th>
              <th scope="col">Favorito</th>
              <th scope="col"></th>
            </tr>
          </thead>
          <tbody>
            <% for(var i = 0; i<contactsBooksL.contacts.length; i++) {  %>
              <tr>
                <td><%= contactsBooksL.contacts[i].name %></td> 
                <td><%= contactsBooksL.contacts[i].email %></td> 
                <td><%= contactsBooksL.contacts[i].mobil %></td> 
                <td><%= contactsBooksL.contacts[i].topList %></td>
                <td><a href="https://contactsbooks.herokuapp.com/deleteContact/<%=contactsBooksL.name%>/<%= contactsBooksL.contacts[i].mobil %>">Eliminar</a></td>  
                
              </tr>
            <% } %>
        
          </tbody>
        </table>
      </div>
      <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-fQybjgWLrvvRgtW6bFlB7jaZrFsaBXjsOMm/tB9LTS58ONXgqbR9W8oWht/amnpF" crossorigin="anonymous"></script>
    </body>
  </html>
