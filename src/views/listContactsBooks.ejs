<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('partials/link');%>
        <title>Libretas de Contacto</title>
</head>

<body>
    <%- include('partials/header'); %>
        <div class="row mt-2">
            <% for(var i=0;i<contactsBooks.length;i++) { %>
                <div class="col-md-2 container">
                    <div class="card border-dark mb-5 justify-center" style="width: 18rem;">
                        <img src="css/contacts1.jpg" class="card-img-top" alt="">
                        <div class="card-body">
                            <h5 class="card-title">
                                <a href="/home/<%= contactsBooks[i] %>" class="btn btn-dark btn-lg btn-block">
                                    <%= contactsBooks[i] %>
                                </a>
                            </h5>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>

    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-fQybjgWLrvvRgtW6bFlB7jaZrFsaBXjsOMm/tB9LTS58ONXgqbR9W8oWht/amnpF" crossorigin="anonymous"></script>
       
</body>

</html>